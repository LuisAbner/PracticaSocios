@model IndexViewModel
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nombre</th>
            <th scope="col">Apellido</th>
            <th scope="col">Salario</th>
            <th scope="col">Dto</th>
            <th scope="col">acciones</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var a in @Model.asociados)
        {
            <tr>
                <th scope="row">@a.IdAsociado</th>
                <td>@a.Nombre</td>
                <td>@a.Apellido</td>
                <td>@a.Salario</td>
                <td>@a.NombreDepartamento</td>
                <td>
                    <button type="button" class="btn btn-warning" onclick="GetAso(@a.IdAsociado)" data-id="@a.IdAsociado"
                        class="btn btn-warning" data-bs-toggle="modal" data-bs-target="#editAsoModal">
                        <i class="bi bi-pencil-square"></i>
                    </button>
                    <button type="button" class="btn btn-danger" onclick="deleteAso(@a.IdAsociado)" data-id="@a.IdAsociado">
                        <i class="bi bi-trash3"></i>
                    </button>
                </td>
            </tr>
        }
    </tbody>
</table>